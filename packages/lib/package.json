{
  "name": "@rfwatson/microdsp-web",
  "version": "0.0.1",
  "description": "A WebAssembly build of the microdsp crate",
  "repository": {
    "type": "git",
    "url": "https://github.com/rfwatson/microdsp-web.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build"
  },
  "main": "./src/main.ts",
  "types": "./wasm/main.d.ts",
  "exports": {
    ".": {
      "import": "./src/main.ts",
      "types": "./wasm/main.d.ts"
    },
    "./wasm/*": "./wasm/*"
  },
  "files": ["src", "wasm"],
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "vite-plugin-wasm": "^3.5.0"
  }
}
